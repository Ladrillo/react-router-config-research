{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "semi": 1,
    "import/no-unresolved": 2
  },
  "env": {
    "es6": true,
    "browser": true
  }
}